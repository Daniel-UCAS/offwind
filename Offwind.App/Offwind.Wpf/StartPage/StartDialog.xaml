        <StackPanel Grid.Row="1" Grid.Column="0" >
            <Button Name="buttonCFD" Style="{StaticResource myButton}">CFD Simulations</Button>
            <Button Name="buttonEngineering" Style="{StaticResource myButton}">Engineering Tools</Button>
        </StackPanel>
        <Border Grid.Row="1" Grid.Column="0" CornerRadius="4" Margin="10" Background="#FFF2F3F4" Visibility="Hidden">
            <StackPanel>
                <Border Name="borderCFD" Style="{StaticResource buttonBorderA}">
                    <Label Name="txtCFD" Content="CFD Simulations" Style="{StaticResource buttonLabelA}" />
                </Border>
                <Border Name="borderEngineering" Style="{StaticResource buttonBorder}" >
                    <Label Name="txtEngineering" Content="Engineering Tools" Style="{StaticResource buttonLabel}" />
                </Border>
            </StackPanel>
        </Border>
        <Canvas Grid.Column="1" Grid.Row="1" Margin="0,0,0,0">
            <Line HorizontalAlignment="Stretch"
                  VerticalAlignment="Center"
                  Stretch="Uniform"
                  StrokeThickness="1"
                  X1="0" Y1="0" X2="0" Y2="132">
                <Shape.Stroke>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                        <GradientStop Color="Transparent" Offset="0" />
                        <GradientStop Color="#007FC4" Offset="0.5" />
                        <GradientStop Color="Transparent" Offset="1" />
                    </LinearGradientBrush>
                </Shape.Stroke>
                <Line.LayoutTransform>
                    <RotateTransform Angle="0"/>
                </Line.LayoutTransform>
            </Line>
            <dxlc:LayoutControl Canvas.Left="10" Canvas.Top="10" Name="layoutControl1" Height="134" Width="115">
                <dxlc:LayoutGroup Orientation="Vertical">
                    <dxlc:LayoutItem Label="Start" Name="descriptionStart">
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem Label="CFD" Name="descriptionCFD">
                    </dxlc:LayoutItem>
                    <dxlc:LayoutItem Label="Engineering" Name="descriptionEngineering">
                    </dxlc:LayoutItem>
                </dxlc:LayoutGroup>
            </dxlc:LayoutControl>
        </Canvas>
        
    </Grid>
</Window>

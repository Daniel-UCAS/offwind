<Window xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
        x:Class="Offwind.StartPage.StartDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Offwind Start"
        Height="370" Width="636"
        WindowStyle="ToolWindow"
        ResizeMode="NoResize"
        xmlns:dxca="http://schemas.devexpress.com/winfx/2008/xaml/carousel"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors">

    <Window.Resources>
        <SolidColorBrush x:Key="mouseOverColor" Color="Red" />
        <SolidColorBrush x:Key="activeButton" Color="White" />
        <SolidColorBrush x:Key="inactiveButton" Color="#4B606C" />
        <Style x:Key="buttonLabel" TargetType="Label">
            <Setter Property="Foreground" Value="{StaticResource inactiveButton}" />
            <Setter Property="FontFamily" Value="Arial" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="10" />
        </Style>
        <Style x:Key="buttonLabelA" TargetType="Label">
            <Setter Property="Foreground" Value="{StaticResource activeButton}" />
            <Setter Property="FontFamily" Value="Arial" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="10" />
        </Style>
        <Style x:Key="myButton" TargetType="Button">
            <Setter Property="Margin" Value="20,20,20,0" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="FontSize" Value="16" />
            <Setter Property="FontFamily" Value="Arial" />
            <Setter Property="Height" Value="46" />
            <Setter Property="Focusable" Value="False" />
            <Setter Property="Background">
                <Setter.Value>
                    <ImageBrush ImageSource="/Offwind.App;component/Images/bg-sidenav.gif" />
                </Setter.Value>
            </Setter>
            <EventSetter Event="MouseEnter" Handler="ButtonMouseEnter" />
            <EventSetter Event="MouseLeave" Handler="ButtonMouseLeave" />
        </Style>
        <Style x:Key="buttonBorder" TargetType="Border">
            <Setter Property="Margin" Value="5" />
            <Setter Property="CornerRadius" Value="5" />
            <Setter Property="Height" Value="Auto" />
            <Setter Property="Width" Value="Auto" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <EventSetter Event="MouseEnter" Handler="borderEngineering_MouseEnter" />
            <EventSetter Event="MouseLeave" Handler="borderEngineering_MouseLeave" />
        </Style>
        <Style x:Key="buttonBorderA" TargetType="Border">
            <Setter Property="Margin" Value="10" />
            <Setter Property="CornerRadius" Value="5" />
            <Setter Property="Height" Value="Auto" />
            <Setter Property="Width" Value="Auto" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Background">
                <Setter.Value>
                    <ImageBrush ImageSource="/Offwind.App;component/Images/bg-sidenav.gif" />
                </Setter.Value>
            </Setter>
            <Setter Property="Effect">
                <Setter.Value>
                    <BlurEffect/>
                </Setter.Value>
            </Setter>
            <EventSetter Event="MouseEnter" Handler="borderEngineering_MouseEnter" />
            <EventSetter Event="MouseLeave" Handler="borderEngineering_MouseLeave" />
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="241*" />
            <ColumnDefinition Width="373*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="78" />
            <RowDefinition Height="283*" />
        </Grid.RowDefinitions>
        <Image Name="imgBg"
               Height="78"
               HorizontalAlignment="Left" Stretch="Fill"
               VerticalAlignment="Top" Source="/Images/bg-banner-holder.png" Grid.ColumnSpan="2" />

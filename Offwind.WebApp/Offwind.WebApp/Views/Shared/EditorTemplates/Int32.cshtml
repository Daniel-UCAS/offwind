@model Int32
@{var fieldName = ViewData.TemplateInfo.HtmlFieldPrefix;}
<div class="control-group">
    <label class="control-label" for="@fieldName">@fieldName <i class="icon-question-sign"></i></label>
    <div class="controls">
        <input type="text" class="inputmask integer" id="@fieldName" name="@fieldName" placeholder="0" value="@Model"/>
    </div>
</div>
@using Offwind.WebApp.Infrastructure
<ul class="breadcrumb">
    <li><a href="@Url.Action("Index", "Home", new { area = "" })"><i class="icon-home"></i></a> <span class="divider">/</span></li>
    <li class="active">Engineering Tools</li>
</ul>
<div class="row-fluid row-content-block">
    <div class="span6 well sub-block-fixed">
        <h2>@Html.BlockTitle("eng-tools-meso-wind-db")</h2>
        @Html.BlockContent("eng-tools-meso-wind-db")
        <a href="@Url.Action("Database", "MesoWind", new { area = "EngineeringTools" })" class="btn btn-primary btn-info">Open</a> <br/> <br/>
    </div>

    <div class="span6 well sub-block-fixed">
        <h2>@Html.BlockTitle("eng-tools-power-calc")</h2>
        @Html.BlockContent("eng-tools-power-calc")
        <a href="@Url.Action("InputData", "WindWave", new { area = "EngineeringTools" })" class="btn btn-primary btn-info">Open</a> <br/> <br/>
    </div>
</div>
<div class="row-fluid row-content-block">
    <div class="span6 well sub-block-fixed">
        <h2>@Html.BlockTitle("eng-tools-wake-sim")</h2>
        @Html.BlockContent("eng-tools-wake-sim")
        <a href="@Url.Action("GeneralProperties", "WakeSimulation", new { area = "EngineeringTools" })" class="btn btn-primary btn-info">Open</a> <br/> <br/>
    </div>

    <div class="span6 well sub-block-fixed">
        <h2>@Html.BlockTitle("eng-tools-wind-farm-control")</h2>
        @Html.BlockContent("eng-tools-wind-farm-control")
        <a href="@Url.Action("Simulation", "WakeSimulation2", new { area = "EngineeringTools" })" class="btn btn-primary btn-info">Open</a> <br/> <br/>
    </div>
</div>
